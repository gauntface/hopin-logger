{
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "ava": "nyc ava ./test/{node,integration}/**/*.js",
    "build": "gulp build",
    "docs": "typedoc ./src/**/*.ts --out ./docs/ --mode file --excludePrivate --excludeProtected --module umd --ignoreCompilerErrors",
    "lint": "tslint 'src/**/*'",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "npm run build && npm run ava && npm run lint"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ]
  },
  "devDependencies": {
    "@hopin/wbt-clean": "0.0.33",
    "@hopin/wbt-ts-browser": "0.0.33",
    "@hopin/wbt-ts-node": "0.0.33",
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^8.0.1",
    "@types/puppeteer": "2.0.0",
    "@types/sinon": "^7.5.1",
    "ava": "^3.3.0",
    "coveralls": "^3.0.9",
    "express": "4.17.1",
    "fs-extra": "8.1.0",
    "glob": "^7.1.6",
    "gulp": "4.0.2",
    "nyc": "15.0.0",
    "puppeteer": "2.1.1",
    "sinon": "8.1.1",
    "tslint": "^6.0.0",
    "typedoc": "0.16.9"
  },
  "peerDependencies": {
    "chalk": "^2.x.x"
  },
  "keywords": [
    "console",
    "log",
    "hoping",
    "logger"
  ],
  "files": [
    "build-node/*",
    "build-browser/*"
  ],
  "homepage": "https://github.com/gauntface/hopin-logger#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gauntface/hopin-logger.git"
  },
  "bugs": {
    "url": "https://github.com/gauntface/hopin-logger/issues"
  },
  "name": "@hopin/logger",
  "description": "A simple console abstraction with some little extras",
  "author": "gauntface",
  "license": "MIT",
  "main": "./build-node/node/index.js",
  "types": "./build-node/node/index.d.ts",
  "browser": "./build-browser/browser/browser.js",
  "version": "2.0.1"
}

{
  "name": "hopin-logger",
  "version": "0.0.1",
  "description": "A simple console abstraction with some little extras",
  "main": "./dist/node/Logger.js",
  "files": [
    "dist/**/*"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "ava": "ava ./build/test/{node,integration}/**/*.js",
    "build": "npm run build:test && npm run build:dist",
    "build:test": "rm -rf ./build/ && tsc -p ./tsconfigs/node-commonjs.json && cp -R ./test/static ./build/test/static/",
    "build:dist": "rm -rf ./dist/ && tsc -p ./tsconfigs/es2015-modules.json",
    "lint": "tslint 'src/**/*'",
    "test": "npm run build && npm run ava && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gauntface/hopin-logger.git"
  },
  "keywords": [
    "console",
    "log",
    "hoping",
    "logger"
  ],
  "author": "gauntface",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gauntface/hopin-logger/issues"
  },
  "homepage": "https://github.com/gauntface/hopin-logger#readme",
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/puppeteer": "^1.0.0",
    "@types/sinon": "^4.1.3",
    "ava": "^0.25.0",
    "express": "^4.16.2",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "gts": "^0.5.3",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.1",
    "puppeteer": "^1.1.1",
    "sinon": "^4.4.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "peerDependencies": {
    "chalk": "^2.x.x"
  }
}
